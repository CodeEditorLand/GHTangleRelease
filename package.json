{
	"name": "gh-tangle-release",
	"version": "0.0.1",
	"private": false,
	"description": "Create a release and adds metadata to the IOTA Tangle",
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/GhTangleRelease#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/GhTangleRelease/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/GhTangleRelease.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Land",
		"email": "Land@Playform.Cloud",
		"url": "HTTPS://Land.Playform.Cloud"
	},
	"type": "module",
	"main": "dist/index.js",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"build": "ncc build src/main.js",
		"precommit": "npm run build && git add dist/",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/tangle-release.js"
		],
		"coverageThreshold": {
			"global": {
				"branches": 0,
				"functions": 0,
				"lines": 0,
				"statements": 0
			}
		},
		"testEnvironment": "node"
	},
	"dependencies": {
		"@actions/core": "1.10.1",
		"@actions/exec": "1.1.1",
		"@actions/github": "6.0.0",
		"@iota/converter": "1.0.0-beta.30",
		"@iota/core": "1.0.0-beta.30",
		"axios": "1.6.8"
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@zeit/ncc": "0.20.5"
	},
	"publishConfig": {
		"access": "public"
	}
}
